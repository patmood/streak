	<p>You're logged in bro!</p>
	<p><%= link_to "Make a new item", new_item_path %></p>
	<p><a href="/signout">Signout</a></p>
	<div id="list">
		<ul>
			<%= form_for @item do |f| %>
			<li><%= f.text_field :description %></li>
			<% end %>
			<% @user.items.each do |item| %>
			<li><%= item.description %></li>
			<% end %>
		</ul>
	</div>